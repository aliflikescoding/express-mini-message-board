{
  "name": "node-express-chat-mini-message-board",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "node --watch --env-file=.env ./bin/www",
    "devstart": "concurrently \"npm run watch:css\" \"nodemon ./bin/www\"",
    "serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart",
    "tailwind:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "watch:css": "nodemon -e ejs,css -x \"npm run tailwind:css\""
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "autoprefixer": "^10.4.19",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.13.1",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  }
}
